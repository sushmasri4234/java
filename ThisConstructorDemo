class A
{
    A()
    {
        System.out.println("Inside default Constructor ");
    }
    A(int x)
    {
        this();
        System.out.println("Inside Parameterised Constructor");
        System.out.println("x value is "+x);
    }
}
class ThisConstructorDemo
{
    public static void main(String args[])
    {
        A a = new A(4234);
    }
}
