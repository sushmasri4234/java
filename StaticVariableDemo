import java.io.*;
class Student
{
    int rollno;
    String name;
    static String college = "SITE";
    Student(int r,String n)
    {
        rollno = r;
        name = n;
    }
    void display()
    {
        System.out.println(rollno+" "+name+" "+college);
    }
}
class StaticVariableDemo
{
    public static void main(String args[])
    {
        Student s1 = new Student(234,"Sush");
        Student s2 = new Student(252,"Bhagya");
        Student s3 = new Student(250,"suma");
        s1.display();
        s2.display();
        s3.display();
    }
}
