import java.io.*;
class CounterDemo
{
    static int count = 0;
    CounterDemo()
    {
        count++;
        System.out.println("Count = "+count);
    }
}
class StaticVariableDemo2
{
    public static void main(String args[])
    {
        CounterDemo cd = new CounterDemo();
        CounterDemo cd1 = new CounterDemo();
        CounterDemo cd3 = new CounterDemo();
    }
}
